PROJECT_NAME           = "@CMAKE_PROJECT_NAME@"
PROJECT_NUMBER         = "@VERSION_MAJOR@.@VERSION_MINOR@.@VERSION_PATCH@"
PROJECT_BRIEF          = "| C++ Documentation"
STRIP_FROM_PATH        = @FAST_SOURCE_DIR@ \
                         @PROJECT_SOURCE_DIR@/doc/pages/
INPUT                  = @PROJECT_SOURCE_DIR@/doc/mainpage.dox \
                         @FAST_SOURCE_DIR@ \
                         @PROJECT_SOURCE_DIR@/doc/pages/
FILE_PATTERNS          = *.hpp, *.dox
EXCLUDE_PATTERNS       = */Examples/* \
                         */Tests/* \
                         */Tools/* 
RECURSIVE              = YES
CREATE_SUBDIRS          = NO
GENERATE_HTML           = NO
GENERATE_XML            = YES
XML_PROGRAMLISTING      = NO
XML_NS_MEMB_FILE_SCOPE  = YES
M_SHOW_UNDOCUMENTED     = YES
EXTRACT_ALL             = YES
M_FAVICON = @PROJECT_SOURCE_DIR@/doc/images/fast_icon.ico
PROJECT_LOGO = @PROJECT_SOURCE_DIR@/doc/images/fast_icon.png
IMAGE_PATH = @PROJECT_SOURCE_DIR@/doc/images/
HTML_EXTRA_STYLESHEET = 'https://fonts.googleapis.com/css?family=Source+Sans+Pro:400,400i,600,600i%7CSource+Code+Pro:400,400i,600' \
    '../css/m-dark+documentation.compiled.css' \
    '@PROJECT_SOURCE_DIR@/doc/fast.css'
M_THEME_COLOR = '#22272e'
EXCLUDE_SYMBOLS = cl, libfreenect2, nvinfer1, tensorflow
M_LINKS_NAVBAR1 = pages annotated
M_LINKS_NAVBAR2 = "<a href=\"https://github.com/smistad/FAST/wiki/\">Wiki</a>" \
                "<a href=\"https://github.com/smistad/FAST/\">GitHub</a>"
